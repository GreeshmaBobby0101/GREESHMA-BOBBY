FILM_QUEUE/////////////////////////










{
  "richContent": [
    [
      {
        "type": "description",
        "title": "How to control the film queue?", 
        "text": [
          "STEP 1:Click [Film Queue] button, the printing queue window opens.",
          "STEP 2:Click [Pause] to pause the selected printing jobs.",
          "STEP 3:Click [Resume] to resume the jobs which be paused.",
          "STEP 4:Click [Refresh] to refresh the printing job list.",
          "STEP 5:Click [Clear] to delete the selected printing jobs.",
          "STEP 6:Click [Close] to close."       
        ]
      }
    ]
  ]
}







